grammar PGN    

game <- tag_pairs:tag_pairs [\n]? movetext:movetext*
tag_pairs <- tag_pair* %make_tag_pairs
tag_pair <- "[" key " \"" value "\"]" dlm %make_tag_pair
key <- [A-Za-z]+
value <- [^\"]*

movetext <- move_number dlm white:move dlm wcomment:comment? dlm black:move dlm bcomment:comment? dlm %make_movetext
move_number <- [0-9]+ "." 
move <- [KQRNBP]? [a-h] [1-8]   
comment <- "{" [^\}]* "}" %make_comment
dlm <- [\s]?
